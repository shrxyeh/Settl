{
  "name": "settl-aml-bot",
  "version": "1.0.0",
  "description": "Settl X AML Telegram Bot - Multi-chain wallet tracking and risk analysis",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "tsx watch src/worker.ts",
    "worker:prod": "node dist/worker.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "heroku-postbuild": "npm run build && npx prisma generate"
  },
  "keywords": [
    "telegram",
    "bot",
    "aml",
    "blockchain",
    "ethereum",
    "solana",
    "tracking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "express": "^4.21.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.6",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
